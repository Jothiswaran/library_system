<table border ="0" cellspacing="20">
<tr align ="left">
<th>Library ID </th>
<th >Library Name </th>
<th>City </th>
<th>Phone</th>
</tr>
<% @libraries.each do |instance| %>
<td> <%= instance.id %> </td>
<td> <%= instance.name %></td>
<td> <%= instance.city %></td>
<td> <%= instance.phone %></td>
</tr>
<% end %>
</table>
 